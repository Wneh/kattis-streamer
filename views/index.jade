extends layout

block content

	h1 Much data Such bootstrap

	table(class="table table-hover")
		thead
			tr
				th ID
				th DATE
				th AUTHOR
				th PROBLEM
				th STATUS
				th CPU
				th LANG
		tbody
			each row in data
				if row.status == "Accepted"
					tr.success
						each col in row
							th #{col}
				else if row.status == "Wrong Answer" || row.status == "Time Limit Exceeded" || row.status == "Run Time Error"
					tr.danger
						each col in row
							th #{col}
				else 
					tr
						each col in row
							th #{col}